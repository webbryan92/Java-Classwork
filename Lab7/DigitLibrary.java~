public class DigitLibrary{
  public static int[] count(String s){
    int[] counts = new int[10];
    for(int i = 0; i < s.length(); i++) {
      if (i = s.charAt(i))
        counts[i]++;
  }
  }
   public static void printArray(int[] Array)
 {
  for(int i = 0; i < Array.length; i ++)
   System.out.println(Array[i]);
 }
}